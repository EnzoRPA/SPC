{
    "functions": {
        "public/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "env": {
        "DB_CONNECTION": "pgsql",
        "DB_PORT": "5432"
    }
}